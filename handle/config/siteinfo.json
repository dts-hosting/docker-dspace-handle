{
  "version": 1,
  "protocolVersion": "2.11",
  "serialNumber": 1,
  "primarySite": true,
  "multiPrimary": false,
  "attributes": [
    {
      "name": "desc",
      "value": "$HANDLE_SERVER_NAME"
    }
  ],
  "servers": [
    {
      "serverId": 1,
      "address": "$HANDLE_HOST_IP",
      "publicKey": {
