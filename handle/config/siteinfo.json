      },
      "interfaces": [
        {
          "query": true,
          "admin": false,
          "protocol": "UDP",
          "port": 2641
        },
        {
          "query": true,
          "admin": false,
          "protocol": "TCP",
          "port": 2641
        },
        {
          "query": true,
          "admin": false,
          "protocol": "HTTP",
          "port": 8000
        }
      ]
    }
  ]
}
